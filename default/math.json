{
  "agent_role": "ci_workflow_16generator",
  "repository": "lakshmi1212/Math1234",
  "default_branch": "feature2",
  "workflow_file": ".github/workflows/ci.yml",
  "python_version": "3.10",
  "dependencies": [
    "pytest==8.0.0"
  ],
  "test_command": "python -m pytest tests/ -v --tb=short",
  "requirements_file": "default/requirements.txt",
  "test_directory": "tests",
  "jobs": [
    {
      "job_id": "ci",
      "job_name": "Run Pytest Suite",
      "runs_on": "ubuntu-latest",
      "steps": [
        {
          "step_name": "Checkout code",
          "action_type": "uses",
          "action": "actions/checkout@v4"
        },
        {
          "step_name": "Set up Python",
          "action_type": "uses",
          "action": "actions/setup-python@v5",
          "with": {
            "python-version": "3.10"
          }
        },
        {
          "step_name": "Install dependencies",
          "action_type": "run",
          "command": "pip install -r default/requirements.txt"
        },
        {
          "step_name": "Run tests",
          "action_type": "run",
          "command": "python -m pytest tests/ -v --tb=short"
        }
      ]
    }
  ]
}

{
  "agent_role": "ci_workflow_16generator",
  "repository": {
    "url": "https://github.com/lakshmi1212/Math1234",
    "branch": "feature2"
  },
  "workflow": {
    "file_path": ".github/workflows/ci.yml",
    "name": "CI Pipeline"
  },
  "triggers": {
    "push": [".github/workflows"],
    "pull_request": ["feature2"]
  },
  "runtime": {
    "language": "python",
    "version": "3.10"
  },
  "dependencies": [
    "pytest==8.0.0"
  ],
  "test_execution": {
    "test_directory": "tests",
    "pytest_command": "pytest tests/ -v --tb=short",
    "fail_on_error": true
  },
  "constraints": {
    "modify_source_files": false,
    "modify_test_files": false
  },
  "commit": {
    "enabled": true,
    "message": "Add CI workflow for Python tests"
  },
  "jobs": [
    {
      "job_id": "ci_test",
      "job_name": "Run Pytest Suite",
      "runs_on": "ubuntu-latest",
      "steps": [
        {
          "step_name": "Checkout repository",
          "action_type": "uses",
          "action": "actions/checkout@v4"
        },
        {
          "step_name": "Set up Python",
          "action_type": "uses",
          "action": "actions/setup-python@v4"
        },
        {
          "step_name": "Install dependencies",
          "action_type": "run",
          "command": "pip install -r default/requirements.txt"
        },
        {
          "step_name": "Run tests",
          "action_type": "run",
          "command": "pytest tests/ -v --tb=short"
        }
      ]
    }
  ]
}
